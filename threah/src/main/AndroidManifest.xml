<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pr.tongson.threah">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainThreadActivity"></activity>
        <activity android:name=".ChildThreadActivity"></activity>
        <activity android:name=".HandlerActivity"></activity>
        <activity android:name=".LooperActivity"></activity>
        <activity android:name=".AsyncTaskActivity"></activity>
        <activity android:name=".JavaThreadActivity"></activity>
    </application>

</manifest>